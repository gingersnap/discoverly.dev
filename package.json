{
  "name": "discoverly.dev",
  "version": "1.0.0",
  "description": "A product discovery tool website built with Eleventy and Tailwind CSS",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:eleventy": "eleventy --serve --quiet",
    "dev:vite": "vite build --watch",
    "build": "run-s clean build:*",
    "build:eleventy": "eleventy",
    "build:vite": "vite build",
    "clean": "rm -rf _site",
    "preview": "run-s build && eleventy --serve"
  },
  "keywords": [
    "eleventy",
    "static-site",
    "product-discovery"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-handlebars": "^1.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "handlebars": "^4.7.8",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.0.0",
    "vite": "^7.0.5"
  },
  "dependencies": {
    "alpinejs": "^3.14.9",
    "lucide": "^0.525.0"
  }
}
